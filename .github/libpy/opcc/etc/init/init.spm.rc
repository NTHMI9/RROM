# permissive
on property:sys.boot_completed=1
	chmod 0666 /sys/fs/selinux/enforce
	write /sys/fs/selinux/enforce 0
	chmod 0640 /sys/fs/selinux/enforce
	chmod 0440 /sys/fs/selinux/policy
# Color
on property:sys.boot_completed=1
    write /sys/module/msm_drm/parameters/kcal_cont 264
    write /sys/module/msm_drm/parameters/kcal_hue 0
    write /sys/module/msm_drm/parameters/kcal_sat 272
    write /sys/module/msm_drm/parameters/kcal_val 255
    write /sys/module/msm_drm/parameters/kcal_red 255
    write /sys/module/msm_drm/parameters/kcal_green 255
    write /sys/module/msm_drm/parameters/kcal_blue 255
