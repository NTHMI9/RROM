# Initialization
on property:init.svc.thermal-engine=running
	stop thermal-engine
	stop logd
	stop vendor.cnss_diag
	stop vendor.tcpdump
	stop vendor.ipacm-diag
	stop cnss_diag
	stop tcpdump
	stop ipacm-diag
	stop logcat
    stop logcatd
    stop tcpdump
    stop statsd
    stop traced
    stop idd-logreader
    stop idd-logreadermain
    stop stats
    stop dumpstate
    stop aplogd
    stop vendor_tcpdump
    stop boot_logo_updater
    stop diag_mdlog_system
    stop logwrapper
    stop bootstat
    stop dmesg
    stop mdnsd
    stop bugreport
    stop mobile_log_d
    stop connsyslogger
    stop debuggerd
    stop vmstat
# Set boot completed
on property:sys.boot_completed=1
	chmod 0755 /system/bin/spm
	chmod 0731 /data/system/theme
	exec u:r:init:s0 root root -- /system/bin/spm 
	exec u:r:sysinit:s0 root root -- /system/bin/spm 
	exec -- /system/bin/spm 
